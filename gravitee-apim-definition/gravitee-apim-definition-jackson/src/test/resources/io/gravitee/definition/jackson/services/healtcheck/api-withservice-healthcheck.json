{
  "id": "my-api",
  "name": "my-team-api",

  "proxy": {
    "context_path": "/team",
    "groups": [
      {
        "name": "group-default",
        "endpoints": [
          {
            "name": "default",
            "target": "http://localhost:8083/myapi",
            "healthcheck": true
          }
        ]
      }],
    "strip_context_path": false
  },

  "paths": {
    "/*": [
    ]
  },

  "properties" : [ {
    "key" : "my_property",
    "value" : "true"
  }, {
    "key" : "my_property2",
    "value" : "123"
  }, {
    "key" : "my_property3",
    "value" : "text"
  } ],

  "services": {
    "health-check": {
      "interval": 60,
      "unit": "SECONDS",
      "enabled": true,
      "request": {
        "uri": "http://se56-2-qlf.mbstech.aec.corp/services/cxf/mobilite",
        "method": "GET",
        "headers": [
          {
            "name": "X-TOTO",
            "value": "my_value"
          }, {
            "name": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": "My content in JSON form"
      },
      "expectation": {
        "assertions": [
          "response.status == 200",
          "#jsonPath(response.body, '$.store.book[0].author')",
          "#jsonPath(response.body, '$.store.book[0].author')"
        ]
      }
    }
  }
}