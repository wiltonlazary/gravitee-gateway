{
  "id": "my-api",
  "name": "my-team-api",

  "proxy": {
    "context_path": "/team",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8083/myapi"
      }
    ],
    "strip_context_path": false
  },

  "paths": {
    "/*": [
    ]
  },

  "properties": {
    "my_property": "true",
    "my_property2": "123",
    "my_property3": "text"
  },

  "services": {
    "dynamic-property": {
      "interval": 60,
      "unit": "badunit",
      "enabled": true,
      "url": "http://my_configuration_url"
    }
  }
}