{
  "id": "my-api",
  "name": "my-api",
  "version": "1",
  "gravitee": "1.0.0",
  "flow_mode": "DEFAULT",
  "proxy": {
    "virtual_hosts": [
      {
        "path": "/my-api"
      }
    ],
    "strip_context_path": false,
    "preserve_host": true,
    "groups": [
      {
        "name": "default-group",
        "endpoints": [
          {
            "name": "default",
            "target": "http://localhost:1234",
            "weight": 1,
            "backup": false,
            "type": "http"
          }
        ],
        "load_balancing": {
          "type": "ROUND_ROBIN"
        },
        "http": {
          "connectTimeout": 5000,
          "idleTimeout": 60000,
          "keepAlive": true,
          "readTimeout": 10000,
          "pipelining": false,
          "maxConcurrentConnections": 100,
          "useCompression": true,
          "followRedirects": false
        }
      }
    ]
  },
  "properties": [],
  "request": {
    "path": "/",
    "method": "GET",
    "body": "request-body",
    "headers" : {
      "X-Gravitee-Transaction-Id" : ["transaction-id"],
      "content-type" : ["application/json"],
      "X-Gravitee-Request-Id" : ["request-id"],
      "accept-encoding" : ["deflate", "gzip", "compress"]
    }
  },
  "response": {
    "body": "response-body",
    "statusCode" : 200,
    "headers" : {
      "transfer-encoding" : ["chunked"],
      "accept-encoding" : ["deflate", "gzip", "compress"]
    }
  }
}