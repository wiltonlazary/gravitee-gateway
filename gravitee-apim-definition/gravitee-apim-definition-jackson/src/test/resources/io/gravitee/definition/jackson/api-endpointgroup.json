{
  "id": "my-api",
  "name": "my-team-api",

  "proxy": {
    "context_path": "/team",
    "groups": [
      {
        "name": "default",
        "http" : {
          "connectTimeout" : 5000,
          "idleTimeout" : 60000,
          "keepAlive" : true,
          "readTimeout" : 10000,
          "pipelining" : false,
          "maxConcurrentConnections" : 100,
          "useCompression" : true,
          "followRedirects" : true
        },
        "endpoints": [
          {
            "name": "endpoint_0",
            "target": "http://host1:8083/myapi",
            "inherit": true
          }, {
            "name": "endpoint_1",
            "target": "http://host2:8083/myapi",
            "http" : {
              "connectTimeout" : 4000,
              "idleTimeout" : 50000,
              "keepAlive" : true,
              "readTimeout" : 9000,
              "pipelining" : false,
              "maxConcurrentConnections" : 90,
              "useCompression" : true,
              "followRedirects" : false
            }
          }
        ]
      }
    ],
    "strip_context_path": false
  },

  "paths": {
    "/*": [
    ]
  }
}